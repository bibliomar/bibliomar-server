create table activity (activity json not null, activity_type smallint not null, time TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, user_username varchar(255) not null, primary key (user_username)) engine=InnoDB;
create table statistics (MD5 varchar(255) not null, views BIGINT DEFAULT 0 not null, topic smallint not null, fictionMetadataReference_ID bigint, scitechMetadataReference_ID bigint, primary key (MD5)) engine=InnoDB;
create table user (username varchar(255) not null, email varchar(255) not null, joinedAt TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, password varchar(255) not null, preMigration bit default false not null, privateProfile bit not null, role smallint not null, userLibrary_username varchar(255) not null, primary key (username)) engine=InnoDB;
create table user_library (username varchar(255) not null, backlog json not null, dropped json not null, finished json not null, reading json not null, to_read json not null, primary key (username)) engine=InnoDB;
alter table activity add constraint FKi82mf8llgc3it8klmlbbmou9k foreign key (user_username) references user (username);
alter table user add constraint FKagmtjjhsrx4humcmeobotlix4 foreign key (userLibrary_username) references user_library (username);
create table activity (activity json not null, activity_type smallint not null, time TIMESTAMP DEFAULT CURRENT_TIMESTAMP not null, user_username varchar(255) not null, primary key (user_username)) engine=InnoDB;
create table description (MD5 CHAR(32) not null, Descr varchar(255), primary key (MD5)) engine=InnoDB;
create table fiction (ID bigint not null, MD5 CHAR(32), Author varchar(255), Coverurl varchar(255), Edition varchar(255), Extension varchar(255), Filesize integer, Language varchar(255), Pages varchar(255), Publisher varchar(255), Series varchar(255), TimeAdded datetime(6), TimeLastModified datetime(6), Title varchar(255), Year varchar(255), primary key (ID)) engine=InnoDB;
create table fiction_description (MD5 CHAR(32) not null, Descr varchar(255), primary key (MD5)) engine=InnoDB;
create table statistics (MD5 varchar(255) not null, views BIGINT DEFAULT 0 not null, topic smallint not null, fictionMetadataReference_ID bigint, scitechMetadataReference_ID bigint, primary key (MD5)) engine=InnoDB;
